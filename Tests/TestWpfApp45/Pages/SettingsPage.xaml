<LibraryCommonPage x:Class="TestWpfApp45.SettingsPage"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:TestWpfApp45="clr-namespace:TestWpfApp45"
                   xmlns:Win32="clr-namespace:Microsoft.Win32;assembly=PresentationFramework"
                   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   Name="AppSettingsPage"
                   Title="Application Settings"
                   Width="800"
                   Height="350"
                   CommonDialogClosing="SettingsPage_OnCommonDialogClosing"
                   Description="This is Settings page which automatically is hosted by LibraryCommonDialog."
                   IsDialog="True"
                   Loaded="SettingsPage_OnLoaded"
                   ShowCancelButton="False"
                   ShowCommandBar="True"
                   mc:Ignorable="d" OkButtonClicked="SettingsPage_OnOkButtonClicked">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

    </Grid>
</LibraryCommonPage>